.container {
  padding: 20rpx;
}

.title {
  font-size: 36rpx;
  font-weight: bold;
  margin-bottom: 20rpx;
}

.subtitle {
  font-size: 30rpx;
  font-weight: bold;
  margin-top: 20rpx;
  margin-bottom: 10rpx;
}

.report-details {
  background-color: #f0f0f0;
  padding: 20rpx;
  margin-bottom: 20rpx;
  border-radius: 10rpx;
}

.report-details text {
  display: block;
  margin-bottom: 10rpx;
}

.photo-evidence {
  margin-bottom: 20rpx;
}

.photo-grid {
  display: flex;
  flex-wrap: wrap;
}

.photo-grid image {
  width: 200rpx;
  height: 200rpx;
  margin-right: 10rpx;
  margin-bottom: 10rpx;
}

.save-button {
  background-color: #4CAF50;
  color: white;
}