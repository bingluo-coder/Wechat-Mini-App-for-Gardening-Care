.container {
  padding: 20rpx;
}

.title {
  font-size: 36rpx;
  font-weight: bold;
  margin-bottom: 20rpx;
}

form {
  display: flex;
  flex-direction: column;
}

input, textarea {
  margin-bottom: 20rpx;
  padding: 10rpx;
  border: 1px solid #ccc;
  border-radius: 5rpx;
}

textarea {
  height: 100rpx;
}

button {
  background-color: #4CAF50;
  color: white;
}